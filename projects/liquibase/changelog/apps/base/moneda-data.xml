<changeSet id="moneda-pyg" author="orco">
    <insert tableName="moneda">
        <column name="codigo" value="PYG"/>
        <column name="nombre" value="Guarani"/>
        <column name="descripcion" value="Moneda Guarani"/>
        <column name="FieldDefinition(name='simbolo', type='CharField', max_length=None, null=True, primary_key=False, unique=False, args={})" value=""/>

    </insert>
    <rollback>
        <delete tableName="moneda">
            <where>codigo = 'PYG'</where>
        </delete>
    </rollback>
</changeSet>

