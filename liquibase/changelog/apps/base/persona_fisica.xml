<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
<changeSet id="create-persona_fisica" author="orco">
    <createSequence sequenceName="persona_fisica_id_seq" startValue="1" />
    <createTable tableName="persona_fisica">
        <column name="id" type="int">
            <constraints primaryKey="true" nullable="false"/>
        </column>
        <column name="is_deleted" type="boolean">
            <constraints nullable="false"/>
        </column>
        <column name="createdby" type="varchar(255)">
        </column>
        <column name="updatedby" type="varchar(255)">
        </column>
        <column name="createdat" type="timestamp">
        </column>
        <column name="updatedat" type="timestamp">
        </column>
        <column name="persona" type="int">
        </column>
        <column name="nombres" type="varchar(255)">
        </column>
        <column name="apellidos" type="varchar(255)">
        </column>
        <column name="fecha_nacimiento" type="date">
        </column>
    </createTable>
    <addDefaultValue tableName="persona_fisica" columnName="id" defaultValueComputed="nextval('persona_fisica_id_seq')" />
</changeSet>
</databaseChangeLog>
