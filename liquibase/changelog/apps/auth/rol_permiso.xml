<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
<changeSet id="create-rol_permiso" author="orco">
    <createSequence sequenceName="rol_permiso_id_seq" startValue="1" />
    <createTable tableName="rol_permiso">
        <column name="id" type="int">
            <constraints primaryKey="true" nullable="false"/>
        </column>
        <column name="is_deleted" type="boolean">
            <constraints nullable="false"/>
        </column>
        <column name="createdby" type="varchar(255)">
        </column>
        <column name="updatedby" type="varchar(255)">
        </column>
        <column name="createdat" type="timestamp">
        </column>
        <column name="updatedat" type="timestamp">
        </column>
        <column name="rol_code" type="int">
            <constraints nullable="false"/>
        </column>
        <column name="permiso_code" type="int">
            <constraints nullable="false"/>
        </column>
    </createTable>
    <addDefaultValue tableName="rol_permiso" columnName="id" defaultValueComputed="nextval('rol_permiso_id_seq')" />
</changeSet>
</databaseChangeLog>
