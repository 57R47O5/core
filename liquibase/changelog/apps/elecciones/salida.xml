<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
<changeSet id="create-salida" author="orco">
    <createSequence sequenceName="salida_id_seq" startValue="1" />
    <createTable tableName="salida">
        <column name="id" type="int">
            <constraints primaryKey="true" nullable="false"/>
        </column>
        <column name="is_deleted" type="boolean">
            <constraints nullable="false"/>
        </column>
        <column name="createdby" type="varchar(255)">
        </column>
        <column name="updatedby" type="varchar(255)">
        </column>
        <column name="createdat" type="timestamp">
        </column>
        <column name="updatedat" type="timestamp">
        </column>
        <column name="campana" type="int">
            <constraints nullable="false"/>
        </column>
        <column name="colaborador" type="int">
            <constraints nullable="false"/>
        </column>
        <column name="fecha" type="date">
            <constraints nullable="false"/>
        </column>
        <column name="estado" type="int">
            <constraints nullable="false"/>
        </column>
    </createTable>
    <addDefaultValue tableName="salida" columnName="id" defaultValueComputed="nextval('salida_id_seq')" />
</changeSet>
</databaseChangeLog>
