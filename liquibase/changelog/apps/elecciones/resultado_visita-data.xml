    <changeSet id="resultado_visita-rechazo" author="orco">
        <insert tableName="resultado_visita">
            <column name="id" valueComputed="nextval('resultado_visita_id_seq')"/>
            <column name="codigo" value="Rechazo"/>
            <column name="nombre" value="Rechazo"/>
            <column name="descripcion" value="ResultadoVisita Rechazo"/>

            <!-- BaseModel -->
            <column name="is_deleted" valueBoolean="false"/>
            <column name="createdat" valueDate="NOW()"/>
            <column name="updatedat" valueDate="NOW()"/>
            <column name="createdby" valueNumeric="null"/>
            <column name="updatedby" valueNumeric="null"/>

            <!-- ConstantModel -->
            <column name="activo" valueBoolean="true"/>
        <column name="valor" value=""/>

        </insert>
        <rollback>
            <delete tableName="resultado_visita">
                <where>codigo = 'Rechazo'</where>
            </delete>
        </rollback>
    </changeSet>

